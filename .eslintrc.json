{
    "plugins": ["react"],
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "es6": true,
        "worker": true,
        "serviceworker": true,
        "jest": true
    },
    "rules": {
        "semi": 0,
        "indent": ["off", 4],
        "constructor-super": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-duplicate-imports": 2,
        "no-this-before-super": 2,
        "no-param-reassign": 2,
        "no-useless-constructor": 2,
        "no-var": 2,
        "prefer-const": 1,
        "prefer-spread": 2,
        "guard-for-in": 0,
        "prefer-template": 1,
        "array-bracket-newline": 2,
        "array-bracket-spacing": 2,
        "block-spacing": 2,
        "camelcase": 2,
        "comma-dangle": 0,
        "comma-spacing": 2,
        "eol-last": 2,
        "no-array-constructor": 2,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 0,
        "no-plusplus": 0,
        "no-tabs": 2,
        "no-ternary": 0,
        "require-jsdoc": 0,
        "valid-jsdoc": 0,
        "quotes": 0,
        "max-len": 0,
        "react/no-string-refs": 1,
        "react/no-find-dom-node": 1,
        "import/first": 0,
        "react/sort-comp": 0,
        "react/jsx-first-prop-new-line": 0,
        "react/jsx-closing-bracket-location": 0,
        "spaced-comment": 0,
        "no-prototype-builtins": 1,
        "space-in-parens": 0,
        "array-bracket-newline": 0,
        "react/self-closing-comp": 0,
        "func-names": 0,
        "no-unused-vars": 2,
        "space-before-function-paren": 0,
        "object-curly-spacing": 0,
        "react/jsx-indent-props": 0,
        "no-underscore-dangle": 0,
        "react/jsx-boolean-value": 0,
        "default-case": 2,
        "arrow-parens": 0,
        "react/prop-types": 0,
        "arrow-body-style": 0,
        "no-case-declarations": 0,
        "eqeqeq": 2,
        "no-eval": 2,
        "no-extra-bind": 1,
        "class-methods-use-this": 0,
        "no-fallthrough": 0,
        "no-multi-spaces": 0,
        "no-multi-str": 2,
        "vars-on-top": 1,
        "no-console": 0,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/jsx-indent": 0
    }
}